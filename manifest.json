{
    "name": "Natural Disaster Predictor",
    "version": "1.0",
    "manifest_version": 2,
	//"content_scripts": [{
    // "js": ["jquery.min.js", "YourJavaScriptFile.js"],
    // "matches": ["http://*/*", "https://*/*"]
 //  }],
    "description": "Displays the risks of various natural disasters in the area near an address found from a housing website",
    "permissions": [
	  "storage",
	  "https://api.openhazards.com/",
	  "https://www.openhazards.com/",
	  "http://api.openhazards.com/",
	  "http://www.openhazards.com/",
	  "http://ajax.googleapis.com/",
	  "http://surge.srcc.lsu.edu/files/globalpeaksurgedb.csv",
	    "https://realtor.com/",
	  "https://trulia.com/",
	  "http://trulia.com/",
	  "http://realtor.com/",
      "declarativeContent"
	],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "devault_icon": {
          "16": "icon.png"
      }
           },
    "icons": {
        "16": "icon.png"
    },
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"

  }
